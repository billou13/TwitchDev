{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Redis": {
    "ConnectionString": "127.0.0.1:6379"
  },
  "Twitch": {
    "Channel": "<CHANNEL_NAME>"
  },
  "TwitchBot": {
    "Username": "<BOT_USERNAME>",
    "Oauth": "<OAUTH_TOKEN>"
  }
}
